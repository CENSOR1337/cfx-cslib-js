{
    "compilerOptions": {
        "target": "es2020",
        "module": "es2020",
        "moduleResolution": "Bundler",
        "lib": [
            "es2020"
        ],
        "types": [
            "@types/node"
        ],
        "declaration": true,
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "strictNullChecks": true,
        "skipLibCheck": true
    }
}